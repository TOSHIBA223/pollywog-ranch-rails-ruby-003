<h1>Create a new frog</h1>
<%= form_for @frog do |f| %>
  <%= f.label :name, 'Name' %>
  <%= f.text_field :name %>
  <%= f.label :color, 'Color' %>
  <%= f.text_field :color %>
  <%= f.label :lives, 'Lives' %>
  <%= f.select :pond_id, @ponds.collect {|pond| [pond.name, pond.id]} %>
  <%= f.submit %> 
<% end %>
<%= link_to "Cancel", frogs_path %>